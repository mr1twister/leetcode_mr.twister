https://leetcode.com/problems/find-the-town-judge/

class Solution {
public:
    int findJudge(int N, vector<vector<int>>& trust) {
        for(int i=1;i<=N;i++){
            int k1=0,k2=0;
            for(int j=0;j<trust.size();j++){
                if(trust[j][0]==i)
                    k1++;
                if(trust[j][1]==i)
                    k2++;
            }
            if(!k1&&k2==N-1)
                return i;
        }
        return -1;
    }
};
