https://leetcode.com/problems/n-th-tribonacci-number/

class Solution {
public:
	int tribonacci(int n) {
		if (n == 0)
			return 0;
		if (n == 1)
			return 1;
		if (n == 2)
			return 1;
		vector<int> p;
		if (n > 2){
			p.push_back(0);
			p.push_back(1);
			p.push_back(1);
			for (int i = 3; i <= n; i++){
				p.push_back(p[i - 1] + p[i - 2] + p[i - 3]);
			}
		}
		return p[n];

	}
};
