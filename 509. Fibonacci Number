class Solution {
public:
    int fib(int N) {
        vector<int> res;
        if(N==0){
            return 0;
        }
        if(N==1){
        return 1;
        }
        if(N==2){
            return 1;
        }
        if(N==3){
            return 2;
        }
        res.push_back(0);
        res.push_back(1);
        for(int i = 2; i<N; i++){
        res.push_back(res[i-2]+res[i-1]);
        }
    return res[N-1]+res[N-2];
    }
};
